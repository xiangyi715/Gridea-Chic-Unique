<!DOCTYPE html>
<html>
<head>
    <%- include('./include/head', { siteTitle: themeConfig.siteName }) %>
    <link rel="stylesheet" href="<%= themeConfig.domain %>/media/live2d/css/live2d.css" />
</head>
<body>
    <div class="wrapper">
        <%- include('./include/header') %>
        <div class="main">
            <div class="container">
                <div class="post-wrap tags">
                    <% site.tags.forEach(function(tag) { %>
                    <a href="<%= tag.link%>">
                        <p class="post-title">
                            <i class="fa fa-tags" aria-hidden="true"> <%= tag.name %></i>
                        </p>
                    </a>
                    <% }); %>
                </div>
            </div>
        </div>
    </div>
    <%- include('./include/footer') %>
    </div>

<script src="https://instant.page/1.2.2" type="module" integrity="sha384-2xV8M5griQmzyiY3CDqh1dn4z3llDVqZDqzjzcY+jCBCk/a5fXJmuZ/40JJAPeoU"></script>
</body>
</html>
