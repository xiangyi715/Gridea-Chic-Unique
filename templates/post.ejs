<style>
	p#back-to-top {
		position: fixed;
		bottom: 50px;
		right: 50px;
	}
	
	p#back-to-top a {
		text-align: center;
		text-decoration: none;
		color: #d1d1d1;
		display: block;
		width: 30px;
	    /*ä½¿ç”¨CSS3ä¸­çš„transitionå±æ€§ç»™è·³è½¬é“¾æ¥ä¸­çš„æ–‡å­—æ·»åŠ æ¸å˜æ•ˆæœ*/
		-moz-transition: color 1s;
		-webkit-transition: color 1s;
		-o-transition: color 1s;
	}
	p#back-to-top a:hover {
		color: #87cefa;
	}
	p#back-to-top a span {
		background: #ffffff url(/media/images/returnTop.png) no-repeat center center;
		border-radius: 6px;
		display: block;
		height: 55px;
		width: 30px;
		margin-bottom: 5px;
	      /*ä½¿ç”¨CSS3ä¸­çš„transitionå±æ€§ç»™<span>æ ‡ç­¾èƒŒæ™¯é¢œè‰²æ·»åŠ æ¸å˜æ•ˆæœ*/
		-moz-transition: background 1s;
		-webkit-transition: background 1s;
		-o-transition: background 1s;
	}
	
	#back-to-top a:hover span {
		background: #87cefa url(/media/images/returnTop.png) no-repeat center center;
	}
</style>

<script>
$(document).ready(function() {
    //é¦–å…ˆå°†#back-to-topéšè—
    $("#back-to-top").hide();
 
    //å½“æ»šåŠ¨æ¡çš„ä½ç½®å¤„äºè·é¡¶éƒ¨100åƒç´ ä»¥ä¸‹æ—¶ï¼Œè·³è½¬é“¾æ¥å‡ºç°ï¼Œå¦åˆ™æ¶ˆå¤±
    $(function() {
        $(window).scroll(function() {
            if ($(window).scrollTop() > 100) {
                $("#back-to-top").fadeIn(1500);
            }
            else {
                $("#back-to-top").fadeOut(1500);
            }
        });
        //å½“ç‚¹å‡»è·³è½¬é“¾æ¥åï¼Œå›åˆ°é¡µé¢é¡¶éƒ¨ä½ç½®
        $("#back-to-top").click(function() {
            $('body,html').animate({
                scrollTop: 0
            },
            500);
            return false;
        });
    });
});
</script>

